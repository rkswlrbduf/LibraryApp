<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ProgressBlock">
        <attr name="lineColor" format="color"/>
        <attr name="lineWidth" format="dimension"/>
        <attr name="countedColor" format="color"/>
        <attr name="unCountedColor" format="color"/>
        <attr name="autoStart" format="boolean"/>
        <attr name="withAnim" format="boolean"/>
        <attr name="currentCount" format="integer"/>
        <attr name="maxCount" format="integer"/>
        <attr name="duration" format="float"/>
        <attr name="startDelay" format="float"/>
    </declare-styleable>

</resources>